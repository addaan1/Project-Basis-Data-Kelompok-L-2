    <connection>Koneksi_Source</connection>
    <sql>
SELECT
    DATE(dates.updated_at) as `date`,
    u.id_user as user_id,
    u.peran as `role`,
    COALESCE(sales.income, 0) as total_income,
    COALESCE(purchases.expense, 0) as total_expense,
    COALESCE(sales.kg_sold, 0) as total_kg_sold,
    COALESCE(purchases.kg_bought, 0) as total_kg_bought,
    (COALESCE(sales.tx_count, 0) + COALESCE(purchases.tx_count, 0)) as transaction_count,
    NOW() as created_at,
    NOW() as updated_at
FROM users u
CROSS JOIN (
    SELECT DISTINCT DATE(updated_at) as updated_at 
    FROM transaksis 
    WHERE status_transaksi IN ('disetujui', 'completed', 'confirmed')
) dates
LEFT JOIN (
    SELECT 
        id_penjual, 
        DATE(updated_at) as date, 
        SUM(jumlah * COALESCE(harga_akhir, harga_awalan, 0)) as income, 
        SUM(jumlah) as kg_sold, 
        COUNT(*) as tx_count
    FROM transaksis
    WHERE status_transaksi IN ('disetujui', 'completed', 'confirmed')
    GROUP BY id_penjual, DATE(updated_at)
) sales ON u.id_user = sales.id_penjual AND DATE(dates.updated_at) = sales.date
LEFT JOIN (
    SELECT 
        id_pembeli, 
        DATE(updated_at) as date, 
        SUM(jumlah * COALESCE(harga_akhir, harga_awalan, 0)) as expense, 
        SUM(jumlah) as kg_bought, 
        COUNT(*) as tx_count
    FROM transaksis
    WHERE status_transaksi IN ('disetujui', 'completed', 'confirmed')
    GROUP BY id_pembeli, DATE(updated_at)
) purchases ON u.id_user = purchases.id_pembeli AND DATE(dates.updated_at) = purchases.date
WHERE sales.income IS NOT NULL OR purchases.expense IS NOT NULL
    </sql>
